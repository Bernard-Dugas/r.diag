#----------------debut variables coordonnees (ne pas modifier)---------------------

def_attribut[ lon; LON; 
              units = degrees_east;
              long_name = longitude;
              standard_name = longitude; ]

def_attribut[ lat; LAT; 
              units = degrees_north;
              long_name = latitude; 
              standard_name = latitude; ]

def_attribut[ xc; xc; 
              units = m;
              long_name = x-coordinate of polar-stereographic projection;
              standard_name= projection_x_coordinate; ]

def_attribut[ yc; yc;
              units = m;
              long_name = y-coordinate of polar-stereographic projection; 
              standard_name= projection_y_coordinate; ]

def_attribut[ rlon; rlon;
              long_name = longitude in rotated pole grid;
              units = degrees;
              standard_name = grid_longitude; ]

def_attribut[ rlat; rlat;
              long_name = latitude in rotated pole grid;
              units = degrees;
              standard_name = grid_latitude; ]

def_attribut[ time; time; 
              long_name = time;
              standard_name = time; ] 

def_attribut[ plev; PRES; 0.01;
              units = Pa;
              long_name = Pressure Level;
              standard_name = air_pressure; ]

def_attribut[ tau; tau;
              long_name = topographic dependent part of the gal-chen height coordinate;
              standard_name = atmosphere_hybrid_height_coordinate;
              definition = tau est egal a (1-ZGC/HTOIT); ]

def_attribut[ eta; eta; 
              long_name = top height dependent part of the gal-chen height coordinate;
              standard_name = atmosphere_hybrid_height_coordinate; 
              formula terms = eta: eta tau: tau ztop: htoit zsurface: h0;
              definition = eta est egal a ZGC/HTOIT; ]

def_attribut[ htoit; htoit;
              units = m;
              long_name = height of the top of the model;
              standard_name=top_model_height; ]

def_attribut[ h0; H0;
              units = m;
              long_name = height of the surface above mean sea level;
              standard_name=height_above_mean_sea_level; ]

def_attribut[ ap; AP;
              units = Pa;
              long_name = vertical coordinate formula term: ap(k); ]

def_attribut[ b; B;
              units = 1;
              long_name = vertical coordinate formula term: b(k); ]


#----------------FIN Variables coordonnees (NE PAS MODIFIER)---------------------

