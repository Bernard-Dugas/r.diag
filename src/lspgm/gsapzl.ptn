*     
*     Revision 1.4  92/06/02  16:35:23  armnrbd
*     Effectuer la desallocation de memoire dynamique a la sortie.
*     
*     Revision 1.3  92/03/17  14:08:10  armnrbd
*     Ajouter codes EXIT dans documentation.
*     
*     Revision 1.1  92/03/11  13:29:55  armnrbd
*     Ajouter la documentation automatique.
*     
*     Revision 1.0  92/02/24  14:41:47  armnrbd
*     Initial revision
*     

*     FEB 29/88 - R.LAPRISE. (MAR 14/89 - F.MAJAESS "FIX 6010 FORMAT")

*-----------------------------------------------------------------------
      use diag_toc, only: GetTOC, LirTOC

      IMPLICIT      none

      INTEGER,      PARAMETER :: MAXLEV = 999
      REAL,         PARAMETER :: PTMIN = pression_au_toit
      INTEGER,      PARAMETER :: HEAD = taille_entete

      CHARACTER     GRTYP,LIGNE*80
      LOGICAL       DEF,BIGF,MODETA,OK,
     +              OK1,OK2,MONOTON,AABB
      CHARACTER(10) CLEVP(MAXLEV),COORD,CHOLD
      REAL          GAMMA,RGAS,RLUP,PREF,R,R0,R2,GZUP,PHISCON,
     +              PHICON,PREF0,PTOIT0,PTOIT,TCON,TOPSCAL,BIGBIN
      CHARACTER(4)  TYPE,NOM,CPHI,CPHIS,CTEMP,CLNSP,PKTYP
      INTEGER       MBUF(HEAD),KBUF(HEAD),LBUF(HEAD),NFIL,IO,
     +              NPL,NSL,I,L,LL,K,M,LEN,LEN1,LEN2,LEN3,MAXW,
     +              MAXW1,MAXW2,MAXW3,NWDS,KPAK,LPAK,MPAK,
     +              NPACK,MAXL,IPHIS,ISL,NSETS,NST,LIM1,
     +              LIM2,TOP,BOT,INC,IER, VKIND, TOC_NBR,
     +              TOC_NK, IG1,IG2, IG10,IG20, BASE,IUN
      REAL(8)       PTOIT8,PREF8,R8

      INTEGER       LG   (MAXLEV+1),LP   (MAXLEV+1)
      REAL          AG   (MAXLEV+1),BG   (MAXLEV+1), AH     (MAXLEV  )
      REAL          SG   (MAXLEV+1),SH   (MAXLEV  ), BH     (MAXLEV  )
      REAL          SIG  (MAXLEV+1),FSIG (MAXLEV+1), DLNSIG (MAXLEV+1)
      REAL          PR   (MAXLEV  ),PRLOG(MAXLEV  ), DFLNSIG(MAXLEV+3)

      NAMELIST     /AABB_NML/ AG,BG

      REAL,         DIMENSION(:), ALLOCATABLE        :: PHI,LNPH
      INTEGER,      DIMENSION(:), ALLOCATABLE        :: IBUF,JBUF
      REAL,         DIMENSION(:), ALLOCATABLE,TARGET :: LNSP,T
      REAL,         DIMENSION(:), POINTER            :: PS,VT

      LOGICAL,      EXTERNAL :: RPBLOC
      CHARACTER(4), EXTERNAL :: GETYP,GETHIC
      CHARACTER(16),EXTERNAL :: GETKINDC
      INTEGER,      EXTERNAL :: GETSIZ,GETSIZ2,GETKIND,
     +                          GETHIGH,FNOM,FCLOS
      EXTERNAL      MACSPE,JCLPNT,GETLIGN,XIT,
     +              BURNF,CMPLBL,CONVPR,COORDAB,GETSET2,
     +              GETFLD2,EAPL,PUTSET2,WRITLEV,RECGET,RECUP2,
     +              STFGZ,TGZEX,LEADBLK,SETKIND,MONVERT

      CHARACTER     NOMPRG*256
      COMMON       /PROGNAM/ NOMPRG

      DATA          DEF   / .FALSE. /
      DATA          MODETA/ .FALSE. /

      DATA        ( CLEVP(L), L=1,16 ) /
     +              '   10','   20','   30','   50',
     +              '   70','  100','  150','  200',
     +              '  250','  300','  400','  500',
     +              '  700','  850','  925',' 1000'
     +                                  /

      DATA          GZUP,   RGAS   /
     +             -6.8E04, 287.   /
*===================================================================

      NOMPRG =
     +'$Source: /home/cvs/RMNLIB/utils/rdiag/lspgm/gsapzl.ptn,v $'

*---------------------------------------------------------------------
***    ACCEDER LES FICHIERS D'E/S.

      NFIL = 7
      CALL JCLPNT( NFIL, 11,12,13, 14,15, 5,6 )
 
      PKTYP = GETYP( 11 )
      PKTYP = PKTYP( 1:2 )

      PTOIT = PTMIN
      PREF  = 80000.
      R     = 1.0

      COORD = 'DEF'
      BIGF  =.FALSE.
      AABB  =.FALSE.

      IF (RPBLOC( ' ',LIGNE ))                                 THEN

***        CHECK THE THE PARAMETRE BLOCK.

          NPL   = 16
          RLUP  = 0.0
          GAMMA = 6.5E-3

          PTOIT = PTMIN

          OK = RPBLOC('KIND',LIGNE)
          IF (OK) COORD = LIGNE

          OK = RPBLOC('PLV',LIGNE)
          IF (OK) READ(LIGNE,0005, END=910,ERR=910,IOSTAT=IO ) NPL

          OK = RPBLOC('A',LIGNE)
          IF (OK) READ(LIGNE,0011, END=910,ERR=910,IOSTAT=IO ) RLUP

          OK = RPBLOC('B',LIGNE)
          IF (OK) READ(LIGNE,0011, END=910,ERR=910,IOSTAT=IO ) GAMMA

          OK = RPBLOC('C',LIGNE)
          IF (OK) READ(LIGNE,0011, END=910,ERR=910,IOSTAT=IO ) PTOIT

          OK = RPBLOC('D',LIGNE)
          IF (OK) READ(LIGNE,0011, END=910,ERR=910,IOSTAT=IO ) PREF

          OK = RPBLOC('I',LIGNE)
          IF (OK) READ(LIGNE,0011, END=910,ERR=910,IOSTAT=IO ) R

          DEF = RPBLOC('DEFAUT',LIGNE)
          IF (DEF .AND. LIGNE == 'AABB') AABB = .TRUE.

      ELSE

***        READ-IN DIRECTIVE CARDS.
 
          CALL GETLIGN( 5,LIGNE,80,OK ) 

          IF (.NOT.OK)                                         THEN
             GOTO 910
          ELSE
             READ( LIGNE, 5010, END=910,ERR=910,IOSTAT=IO ) 
     +                          NPL,RLUP,GAMMA,COORD,PTOIT,
     +                          PREF,R
          END IF  

      END IF  

      IF (NPL.LT.0)                                            THEN
          NPL  = -NPL
          BIGF =.TRUE.
      END IF

      IF (COORD.NE.' ') CALL LEADBLK( COORD )
      IF (COORD.EQ.' ') COORD = 'ET15'

      IF (NPL.GT.MAXLEV) CALL                      XIT(' Gsapzl ',-1 )

      IF (PKTYP.EQ.'SQ')                                       THEN

          IF (COORD.EQ.'DEF'  .OR. 
     +        COORD.EQ.'GEM2' .OR. 
     +        COORD.EQ.'GEM3')
     +        CALL SETPT( PTOIT,PREF,R )

          IF (COORD /= 'DEF')                                  THEN
              if (COORD == 'GEM'   .or.
     +            COORD == 'GEM2') CALL setkind('SIGMA')
              if (COORD == 'GEM3'  .or.
     +            COORD == 'GEM4') CALL setkind('HYBRID')
          END IF

      END IF

      IF (.NOT.(DEF .AND. (NPL.EQ.16)))                        THEN

          IF (BIGF)                                            THEN
              DO  K=1,(NPL-1)/8+1
                  CALL GETLIGN( 5,LIGNE,80,OK ) 

                  IF (.NOT.OK)                                 THEN
                      GOTO 910
                  ELSE
                      LIM1 = 8*(K-1)+1
                      LIM2 = MIN( NPL, 8*K )
                      READ( LIGNE, 5030,END=911,ERR=911,IOSTAT=IO) 
     +                                  (CLEVP(I),I=LIM1,LIM2)
                  END IF  
              END DO
          ELSE
              DO  K=1,(NPL-1)/16+1
                  CALL GETLIGN( 5,LIGNE,80,OK ) 

                  IF (.NOT.OK)                                 THEN
                      GOTO 910
                  ELSE
                      LIM1 = 16*(K-1)+1
                      LIM2 = MIN( NPL, 16*K )
                      READ( LIGNE, 5020,END=911,ERR=911,IOSTAT=IO) 
     +                                  (CLEVP(I),I=LIM1,LIM2)
                  END IF  
              END DO
          END IF

          CALL BURNF

      END IF

***    CHECK LP CODING.

      DO  K=1,NPL
          IF (INDEX( CLEVP(K),'.' ).EQ.0)                      THEN
              READ( CLEVP(K),10, ERR=911,END=911,IOSTAT=IO ) LP(K)
              CALL CONVPR( LP(K), PR(K), VKIND, -2 )
              IF (PKTYP.EQ.'SQ')                               THEN
                  IF (VKIND.NE.2) CALL             XIT(' Gsapzl ',-13)
                  CALL CONVPR( LP(K),PR(K),VKIND, +2 )
              END IF
          ELSE
             READ( CLEVP(K),11, ERR=911,END=911,IOSTAT=IO ) PR(K)
             IF (PKTYP.EQ.'SQ')                                THEN
                 VKIND = 2
                 CALL CONVPR( LP(K), PR(K), VKIND, +2 )
             ELSE
                 CALL LVCODE( LP(K), PR(K)/1000.,1 ) 
             END IF
          END IF
      END DO

***    GET SIZE FROM FIRST SET OF BUFFERS.
***    GET ETA VALUES FROM THE GSFLD FILE.
 
      MAXW3 = GETSIZ(  13, MBUF,LEN3,NWDS,MPAK )
      MAXW2 = GETSIZ(  12, LBUF,LEN2,NWDS,LPAK )
      MAXW1 = GETSIZ2( 11, KBUF,LEN1,NWDS,KPAK, LG,NSL )

      IF (MAXW3.EQ.0) CALL                         XIT(' Gsapzl ',-3 )
      IF (MAXW2.EQ.0) CALL                         XIT(' Gsapzl ',-4 )
      IF (MAXW1.EQ.0 .OR. NSL.LT.   1    .OR.
     +                    NSL.GT.MAXLEV) CALL      XIT(' Gsapzl ',-5 )

      IF (PKTYP.EQ.'SQ' .AND. .NOT.AABB)                       THEN

***        RETREIVE VKIND FROM GETSIZ2( 11 ).

          VKIND = GETKIND()

***        TRY LOOKING FOR A HY RECORD.

          CALL LIRPT( 11 )
          CALL GETPT( 11 , PTOIT0,PREF0,R0 )

          IF (PTOIT0 == -1.)                                   THEN

***            LITPT/GETPT DID NOT WORK.
***            SO NOW TRY FOR A !! RECORD.

              CALL LirToc( 11, TOC_NBR )

              IF (TOC_NBR > 0)                                 THEN

                  GRTYP = GETHIC('GRTYP',KBUF )

                  IF (GRTYP /= 'Z')                            THEN
                      IG10 = -1 ; IG20 = -1
                  ELSE
                      IG10  = GETHIGH('IG1',KBUF )
                      IG20  = GETHIGH('IG2',KBUF )
                  END IF

                  CALL GETTOC( 11, 'VER',VKIND, IG10,IG20 )
                  CALL GETTOC( 11, 'NK',TOC_NK, IG10,IG20 )
                 
              END IF

          END IF

          IF (VKIND /= 1    .AND.
     +        VKIND /= 5    .AND.
     +        VKIND /= 5001 .AND.
     +        VKIND /= 5002 .AND.
     +        VKIND /= 5003)                                   THEN
              CALL                                 XIT('  Gsapl ',-14)
          ELSE IF (VKIND == 1)                                 THEN
              IF (COORD.EQ.'DEF')  COORD = 'GEM3'
              IF (COORD.EQ.'PRES') CALL            XIT('  Gsapl ',-16)
          ELSE IF (VKIND == 5 .OR.
     +             VKIND == 5001)                              THEN
              IF (COORD.EQ.'DEF')  COORD = 'GEM2'
              IF (COORD.NE.'GEM2') CALL            XIT('  Gsapl ',-16)
          ELSE IF (VKIND == 5002 .OR. VKIND == 5003)           THEN
              IF (COORD.EQ.'DEF')  COORD = 'GEM4'
              IF (COORD.NE.'GEM4') CALL            XIT('  Gsapl ',-16)
          END IF

          IF (VKIND == 5001)                                   THEN
              CALL GETTOC( 11, 'PTOP' ,PTOIT8, IG10,IG20 )
              CALL GETTOC( 11, 'PREF' ,PREF8 , IG10,IG20 )
              CALL GETTOC( 11, 'RC_1' ,R8    , IG10,IG20 )
              PTOIT0 = PTOIT8 ; PREF0 = PREF8 ; R0 = R8
          END IF

      ELSE IF (COORD.EQ.'DEF')                                 THEN

          IF (PKTYP == 'SQ')                                   THEN
              COORD  = 'GEM'
          ELSE
              COORD  = 'ET15'
          END IF

      END IF

      IF (AABB .AND. COORD == 'GEM4') CALL         XIT('  Gsapzl ',-19)

***    THE DEFAULT VALUE FOR PTOIT DEPENDS ON COORD.

      IF (COORD.EQ.'SIG')                                      THEN 
          PTOIT = MAX( PTOIT,0.00 ) 
      ELSE IF (COORD(1:3).NE.'GEM')                            THEN
          PTOIT = MAX( PTOIT, PTMIN ) 
      ENDIF 

      IF (COORD /= 'GEM2' .AND.
     +    COORD /= 'GEM3' .AND.
     +    COORD /= 'GEM4')                                     THEN
          WRITE(6,6010) RLUP,GAMMA,COORD,PTOIT
      ELSE IF (COORD == 'GEM4')                                THEN
          WRITE(6,6010) RLUP,GAMMA,COORD,PTOIT
          WRITE(6,6012) PREF,R,R2
      END IF
       IF (COORD.EQ.'SIG')                                      THEN
          PTOIT = MAX( PTOIT,0.00 )
      ELSE
          PTOIT = MAX( PTOIT,PTMIN )
      END IF

      CALL WRITLEV( PR,NPL,'PRES' )

***     CHECK THAT PR IS MONOTONIC AND INCREASING.

      CALL MONVERT( PR,NPL, TOP,BOT,INC, MONOTON )

      IF (.NOT.MONOTON .OR. TOP.NE.1) CALL         XIT(' Gsapzl ',-2 )

***    PRESSURE IN GEM: PASCAL.

                                TOPSCAL =   1.0
      IF (COORD(1:3).EQ.'GEM' ) TOPSCAL = 100.0

      DO  L=1,NPL
          PR(L)    = PR(L)*TOPSCAL   
          PRLOG(L) = LOG( PR(L) )
      END DO
 
***    COMPARE FIRST SETS.

      CALL CMPLBL( 0,MBUF, 0,KBUF, OK1 ) 
      CALL CMPLBL( 0,LBUF, 0,KBUF, OK2 ) 

      OK = OK1.AND.OK2

      IF (.NOT.OK)                                             THEN
          CALL PRTLAB( KBUF )
          IF (.NOT.OK1) CALL PRTLAB( MBUF )
          IF (.NOT.OK2) CALL PRTLAB( LBUF )
          CALL                                     XIT(' Gsapzl ',-6 ) 
      END IF

***    CHECK NAME CONSISTENCY AND DEFINE CONVERSION CONSTANTS.

      IF (COORD(1:3).EQ.'GEM' )                                THEN

          WRITE(CTEMP,4) KBUF(3)
          WRITE(CPHIS,4) LBUF(3)
          WRITE(CLNSP,4) MBUF(3)

          IF     ((CTEMP == 'VT' .AND. PKTYP == 'SQ') .OR.
     +            (CTEMP == 'TT'))                             THEN 
              CPHI   = 'GZ'
              TCON   = 273.15
              PHICON = 98.0665
              PHISCON = 98.0665
              IF (CPHIS == 'ME') PHISCON = 9.80665
              IF (CLNSP /= 'LP')                               THEN
                  WRITE(6,6035) CLNSP,'LP'
                  CALL                             XIT(' Gsapzl ',-12) 
              END IF
              IF (CPHIS /= 'MT' .and. CPHIS /= 'ME')           THEN
                  WRITE(6,6035) CPHIS,'ME or MT'
                  CALL                             XIT(' Gsapzl ',-12) 
              END IF
          ELSE IF((CTEMP == 'VT' .AND. PKTYP /= 'SQ') .OR.
     +            (CTEMP == 'TEMP'))                           THEN
              CPHI   = 'PHI'
              TCON   = 0.0
              PHICON = 1.0
              IF (CLNSP /= 'LNSP')                             THEN
                  WRITE(6,6035) CLNSP,'LNSP'
                  CALL                             XIT(' Gsapzl ',-7 ) 
              END IF
              IF (CPHIS /= 'PHIS')                             THEN
                  WRITE(6,6035) CPHIS,'PHIS'
                  CALL                             XIT(' Gsapzl ',-12) 
              END IF
          ELSE
              WRITE(6,6035) CTEMP,'VT, TT or TEMP'
              CALL                                 XIT(' Gsapzl ',-12) 
          END IF

          IF (CTEMP /= 'VT') WRITE(6,6050)

      ELSE

          WRITE(CPHI ,4) KBUF(3)
          WRITE(CPHIS,4) LBUF(3)
          WRITE(CLNSP,4) MBUF(3)

          IF (CPHI.EQ.'GZ')                                    THEN
              CTEMP  = 'TT'
              TCON   = 273.15
              PHICON = 98.0665
              PHISCON = 98.0665
              IF (CPHIS == 'ME') PHISCON = 9.80665
              IF ((CPHIS /= 'MT'  .and. 
     +             CPHIS /= 'ME') .or.
     +             CLNSP /= 'LP')                              THEN
                  WRITE(6,6035) CLNSP,'LP'
                  WRITE(6,6035) CPHIS,'ME or MT'
                  CALL                             XIT(' Gsapzl ',-12) 
              END IF
          ELSE IF (CPHI.EQ.'PHI')                              THEN
              CTEMP  = 'TEMP'
              TCON   = 0.0
              PHICON = 1.0
              IF (CPHIS.NE.'PHIS' .AND. CLNSP.NE.'LNSP')       THEN
                  WRITE(6,6035) CLNSP,'LNSP'
                  WRITE(6,6035) CPHIS,'PHIS'
                  CALL                             XIT(' Gsapzl ',-7 ) 
              END IF
          ELSE
              WRITE(6,6035) CPHI,'GZ or PHI'
              CALL                                 XIT(' Gsapzl ',-12) 
          END IF

      END IF

      LEN  = MAX( LEN1, LEN2, LEN3  )
      MAXW = MAX( MAXW1,MAXW2,MAXW3 )

      DO  L=1,NSL
         CALL CONVPR( LG(L), SG(L), VKIND, -2 )
         IF (VKIND.EQ.2) SG(L) = SG(L) * 0.001
      END DO

***    CHECK THAT SG IS MONOTONIC AND INCREASING.

      CALL MONVERT( SG,NSL, TOP,BOT,INC, MONOTON )

***    GENERATE THE A*,B* TERMS THAT ARE USED TO
***    DETERMINE THE LOCAL PRESSURE VALUES.

      IF (.NOT.AABB)                                           THEN

          IF (.NOT.MONOTON .OR. TOP.NE.1) CALL     XIT(' Gsapzl ',-2 )

          IF (COORD.EQ.'GEM2' .OR.
     +        COORD.EQ.'GEM3' )                                    THEN

              IF (PKTYP.EQ.'SQ')                                   THEN

***            CHECK IF GSFLD HOLDS OTHER
***            VALUES OF PTOIT,PREF AND R.

                  IF (PTOIT0.NE.-1.)                               THEN
                      PTOIT = PTOIT0
                      PREF  = PREF0
                      R     = R0
                  END IF
                  
              END IF

              IF (.NOT.MODETA .AND. COORD.EQ.'GEM3')               THEN

                  MODETA =.TRUE.

                  SG(1:NSL) = SG(1:NSL)
     +                      + ( 1.0 - SG(1:NSL) ) * PTOIT / PREF

              END IF

              WRITE(6,6010) RLUP,GAMMA,COORD,PTOIT
              WRITE(6,6011) PREF,R

              CALL GENAB( AG,BG, SG, PTOIT,PREF,R, NSL )

              IF ((1.0-SG(TOP)*PREF/PTOIT) .GT. 1.E-7)            THEN
                  WRITE(6,6018) (PTOIT/PREF)-SG(TOP)
                  CALL                             XIT('  Gsapl ',-18)
              END IF

          ELSE IF (COORD(1:3).NE.'GEM')                            THEN

***            CONVERT FULL TO HALF ETA LEVELS CONSISTENTLY WITH PAKGCMH.
 
              DO  L=1,NSL
                  SH(L) = SQRT( SG(L)*SG(L+1) )
              END DO
 
***            DEFINE PARAMETERS OF THE HYBRID VERTICAL DISCRETIZATION.
 
              CALL COORDAB( AH,BH, NSL  ,SH,COORD,PTOIT )
              CALL COORDAB( AG,BG, NSL+1,SG,COORD,PTOIT )
 
***            BECAUSE CCC CANNOT CODE OTHERWISE THAN IN PRESSURE
***            SUPPOSE THAT LG = 1 IS THE SURFACE...

              IF (VKIND.EQ.2 .AND. LG(NSL).EQ.1) SG(NSL) = 1.0

              CHOLD = COORD ; IF (AABB) COORD = GETKINDC( VKIND )
              CALL WRITLEV( SG,NSL,COORD ) ; COORD = CHOLD

              SG(NSL+1)= 1.

          ELSE

***            For GEM, PR(i,l) = PTOIT + ETA(l) * ( PS(i) - PTOIT ), so...

              AG(1:NSL) = PTOIT*(1.0-SG(1:NSL))
              BG(1:NSL) = SG(1:NSL)

          END IF

      ELSE

***        AG and BG are read from namelist file 'aabb.nml'.

          IUN = 0 ; IO = 0
          IF (FNOM( IUN, 'aabb.nml', 'SQE+OLD', 0 ) == 0)      THEN

              BIGBIN = -524288. ! -(2^^9)
              ! BIGBIN = BIGBIN*BIGBIN*BIGBIN
              AG(:) = BIGBIN ; BG(:) = BIGBIN

              READ( IUN, NML=AABB_NML, END=0920,ERR=0920,IOSTAT=IO )
              IER = FCLOS( IUN )

              LL = 0 ; DO  L=1,MAXLEV+1
                  IF (AG(L) == BIGBIN .OR. BG(L) == BIGBIN) EXIT
                  LL = LL+1
              END DO

              ! Check the number of valid (AG,BG) pairs.

              IF (LL /= NSL .AND. LL /= NSL-1)                 THEN
                  WRITE(6,6021) NSL,LL
                  CALL                             XIT(' Gsapzl ',-21) 
              ELSE IF (LL == NSL-1)                            THEN
                  AG(LL+1) = 0. ; BG(LL+1) = 1.0 ! Add surface values.
              END IF

              WRITE(6,6110)

          ELSE
              GOTO 0920
          END IF

      END IF

***    ALLOCATE WORKING MEMORY.

      WRITE(TYPE,4) KBUF(1)

      IF (TYPE.EQ.'ZONL') NWDS = KBUF(5)

      IF (TYPE.NE.'GRID' .AND.
     +    TYPE.NE.'ZONL' .AND.
     +    TYPE.NE.'SUBA') CALL                     XIT('  Gsapl ',-19) 

      MAXL = MAX( NSL,NPL )

      ALLOCATE( PHI (NWDS*(MAXL+1)),
     +          T   (NWDS* MAXL),
     +          LNPH(NWDS* NSL),
     +          LNSP(NWDS),
     +          IBUF(LEN),
     +          JBUF(LEN), STAT=IER )

      IF (IER /= 0) CALL                           XIT(' Gsapzl ',-15)

      PS => LNSP ! PS points to LNSP
      VT => T    ! VT points to T

      T(:) = -TCON ; PHI(:) = 0.0

***    READ MOUNTAINS AND KEEP IN JCOM.
 
      CALL RECGET( 12, 'GRID'//'SUBA',-1,CPHIS,-1, JBUF,MAXW,OK )

      IF (.NOT.OK) CALL                            XIT(' Gsapzl ',-4 )

      CALL PRTLAB( JBUF )

***    ADDRESS FOR SURFACE GEOPOTENTIAL IN ARRY PHI.
 
      IPHIS = NSL*NWDS+1
      if (COORD(1:3) == 'GEM') IPHIS = IPHIS-NWDS 

*---------------------------------------------------------------------
***    GET NEXT SET OF GEOPOTENTIALS INTO ARRAY PHI.
 
      NSETS = 0
  150 CONTINUE
      IF (COORD(1:3) == 'GEM')                                 THEN
          CALL GETSET2( 11, VT,  LG,ISL, IBUF,MAXW,OK )
      ELSE
          CALL GETSET2( 11, PHI, LG,ISL, IBUF,MAXW,OK )
      END IF

          IF (NSETS.EQ.0) CALL PRTLAB( IBUF ) 

          IF (.NOT.OK)                                         THEN
              WRITE(6,6030) NSETS
              CALL                                 XIT(' Gsapzl ', 0 )
          END IF

          WRITE(NOM,4) IBUF(3)

          IF ((COORD(1:3) /= 'GEM' .AND. NOM /= CPHI)
     +   .OR. (COORD(1:3) == 'GEM' .AND. NOM /= CTEMP))
     +        CALL                                 XIT(' Gsapzl ',-7 )
          IF (ISL.NE.NSL)  CALL                    XIT(' Gsapzl ',-8 )

          CALL CMPLBL( 0,IBUF, 0,JBUF, OK )

          IF (.NOT.OK)                                         THEN
              CALL PRTLAB( IBUF ) ; CALL PRTLAB( JBUF )
              CALL                                 XIT(' Gsapzl ',-6 )
          END IF

          NPACK = IBUF(8)

***        CHECK FOR (VERTICAL) REFERENCE RECORD CHANGES.

          IF (PKTYP == 'SQ' .AND. TOC_NBR > 0)                 THEN

              IG1   = GETHIGH('IG1' ,IBUF )
              IG2   = GETHIGH('IG2' ,IBUF )

              IF (GRTYP == 'Z')
     +            CALL GETTOC( 11, 'VER',VKIND, IG1,IG2 )

              IF (VKIND == 5001 .and. GRTYP == 'Z')            THEN
                  CALL GETTOC( 11, 'PTOP' ,PTOIT8, IG1,IG2 )
                  CALL GETTOC( 11, 'PREF' ,PREF8 , IG1,IG2 )
                  CALL GETTOC( 11, 'RC_1' ,R8    , IG1,IG2 )
                  PTOIT0 = PTOIT8 ; PREF0 = PREF8 ; R0 = R8
                  IF (PTOIT0 /= PTOIT .or.
     +                PREF0  /= PREF  .or.
     +                R0     /= R     )                        THEN
                      WRITE(6,6017)
                      CALL                         XIT(' Gsapzl ',-17) 
                  END IF
              END IF

          END IF

***        ACCOUNT FOR TEMPERATURE IN CENTIGRADES
***        AND FOR GEOPOTENTIAL IN DAM.

          DO  I=1,NWDS*NSL
              VT (I) = VT (I) + TCON
              PHI(I) = PHI(I) * PHICON
          END DO

***        TRANSFER MOUNTAINS FROM JBUF TO LAST LEVEL OF PHI.

          CALL RECUP2( PHI(IPHIS),JBUF )

          IF (CPHIS == 'ME' .OR. CPHIS == 'MT')                 THEN
              DO  I=0,NWDS-1
                  PHI(IPHIS+I) = PHI(IPHIS+I) * PHISCON
              END DO
          END IF

***        READ LN(SF PRES) FOR THIS STEP.
 
          NST = IBUF(2)
          CALL GETFLD2( 13, LNSP, 'GRID'//'SUBA',NST,CLNSP,-1, 
     +                               IBUF,MAXW,OK )

          IF (NSETS.EQ.0) CALL PRTLAB( IBUF )

          IF (.NOT.OK) CALL                        XIT(' Gsapzl ',-9 )

          IF (COORD(1:3) == 'GEM' .and. COORD /= 'GEM4')       THEN

***            Compute hydrostatic GZ from P0 and VT on model levels.

!!!           if (COORD == 'GEM')                              THEN

              PS(:) = 100.*EXP( LNSP(:) ) ! Define PS in Pascals
              CALL d_p0vt2gz_hyb( PHI, AG, BG, PS, VT,   
     $                            NWDS, NSL, .false., .false. )

!!!           else
!!!
!!!               LNSP(:) = LOG(100.) + LNSP(:) ! Convert LNSP from MB to Pascal
!!!               CALL d_p0vt2gz_hyb( PHI, AG, BG, LNSP, VT,   
!!!  $                                NWDS, NSL, .true., .false. )
!!!               PS(:) = EXP( LNSP(:) )
!!!
!!!           end if

              DO  L=1,NSL
                  BASE = (L-1)*NWDS
                  DO  I=1,NWDS
                      LNPH(BASE+I) = LOG( AG(L) + BG(L) * PS(I) )
                  END DO
              END DO

***            Do interpolations to pressure using 'GEM' routines.

              CALL GEMAPL( VT,LNPH,PR,
     +                     NWDS,NPL,NSL,
     +                     TOP,BOT,INC )

              CALL GEMAPL( PHI, LNPH,PR,
     +                     NWDS,NPL,NSL,
     +                     TOP,BOT,INC )

***            HYDROSTATICALLY CORRECT BOTH T AND GZ UNDER THE GROUND.

              CALL TGZEX2( PHI,T,PS, NWDS,PR ,NPL,GAMMA )

          ELSE IF (COORD == 'GEM4')                            THEN

              print *,'Code manquant a ce jour. Contacter BD...'
              call xit(' Gsapzl ', -101)

          ELSE

***            COMPUTE T ON HALF LEVELS FROM "PHI" ON FULL LEVELS IN
***            A WAY THAT RECOVERS THE  CORRECT TEMPERATURES ON  ETA 
***            COORDINATES.  THEN RECOMPUTE  THE CORRECT  PHI IN ETA
***            COORDINATE,  INTERPOLATE T ON ETA LEVELS TO  PRESSURE 
***            LEVELS, IN-PLACE.
 
              CALL STFGZ ( T,  PHI,NWDS,NSL+1,SG,RGAS )
              CALL ETAPHI( PHI,T,  LNSP,
     +                     AG,BG,RGAS,NWDS,NSL,NSL+1,SIG )
              CALL EAPL  ( T, NWDS,PRLOG,NPL,  
     +                     T,SIG,NSL, LNSP,RLUP,0.,
     +                     AH,BH,NSL+1,FSIG,DFLNSIG,DLNSIG )
 
***            INTERPOLATE PHI FROM ETA TO PRESSURE, IN-PLACE.
***            EXTRAPOLATION USES ASSUMED DERIVATIVES GZUP,GZDN.
***            THEN CORRECT BOTH T AND GZ UNDER THE GROUND.
 
              CALL EAPL ( PHI, NWDS, PRLOG, NPL, 
     +                    PHI, SIG,  NSL+1, LNSP,  GZUP,0.,
     +                    AG,BG,   NSL+2,FSIG,  DFLNSIG, DLNSIG )
              CALL TGZEX( PHI, T,LNSP,NWDS,PR ,NPL,GAMMA )
 
          END IF

***        ACCOUNT FOR TEMPERATURE IN CENTIGRADES
***        AND FOR GEOPOTENTIAL IN DAM.

          DO  I=1,NWDS*NPL
              VT (I) = VT (I) - TCON
              PHI(I) = PHI(I) / PHICON
          END DO

***        CHANGE IP1 CODING TO MB FOR STANDARD FILES.

          IF (PKTYP(1:2).EQ.'SQ') CALL SETKIND('PRESSION')

***        WRITE-OUT PRESSURE LEVELS PHI AND TEMP.
 
          READ(CPHI,4) IBUF(3)
          IBUF(8) = NPACK
          CALL PUTSET2( 14, PHI, LP,NPL, IBUF,MAXW )

          IF (NSETS.EQ.0) CALL PRTLAB( IBUF )

          READ(CTEMP,4) IBUF(3)
          CALL PUTSET2( 15, VT, LP,NPL, IBUF,MAXW )
          IF (NSETS.EQ.0) CALL PRTLAB( IBUF )

      NSETS = NSETS+1
      GOTO 150
 
***    E.O.F. ON INPUT.
 
 0910 CALL                                         XIT(' Gsapzl ',-10 )
 0911 CALL                                         XIT(' Gsapzl ',-11 )
 0920 WRITE(6,6020) IO
      CALL                                         XIT(' Gsapzl ',-20 )

*---------------------------------------------------------------------

 0004 FORMAT(A4)
 0005 FORMAT(BN,I5)
 0010 FORMAT(BN,I10)
 0011 FORMAT(BN,E10.0)

 5010 FORMAT(10X,I5,2E10.0,1X,A4,3E10.0)
 5020 FORMAT(16A5)
 5030 FORMAT(8A10)

 6010 FORMAT('0 Rlup,gamma =',2E12.3,', coord=',1X,A4,
     1       ', p.lid (PA)=',F10.3)
 6011 FORMAT(' P.REF=',F10.3,', R=',F10.3)
 6012 FORMAT(' P.REF=',F10.3,', R=',2F10.3)

 6017 FORMAT(' More that one input vertical coordinate.'/) 
 6018 FORMAT('  (P.LID/P.REF)-ETA(TOP)=',E12.4)
 6020 FORMAT(' Unable to read AG,BG in aabb.nml namelist file,',
     +       ' error no.',I5/)
 6021 FORMAT(' Wrong number of AG, BG entries in AABB namelist.',
     +       ' Expecting ',I3,' of each, but found ',I3,'.'/)

 6030 FORMAT(1X,I5,' sets interpolated.')
 6035 FORMAT('0 Found ',A,' while expecting ',A)
 6050 FORMAT(/10x,'=================================================='/
     +        10x,'Looking for virtual temperature, found temperature'/
     +        10x,'=================================================='/)

 6110 FORMAT(' Using AG and BG from file aabb.nml'/)

      END
